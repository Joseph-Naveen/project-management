# Example Frontend Environment Configuration
# Copy this file to .env and update the values for your local development

# API Configuration (Uses Vite proxy for local dev, full URL for production)
VITE_API_BASE_URL=/api

# Application Configuration
VITE_APP_NAME=Project Management Dashboard
VITE_APP_VERSION=1.0.0

# Development Configuration
NODE_ENV=development

# Optional: Feature Flags
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_DEBUG=true
